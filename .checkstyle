  <local-check-config name="maven-checkstyle-plugin test" location="https://raw.githubusercontent.com/ONSdigital/rm-common-config/master/checkstyle/ons-checkstyle-config.xml" type="remote" description="maven-checkstyle-plugin configuration test">
    <property name="checkstyle.cache.file" value="${project_loc}/target/checkstyle-cachefile"/>
